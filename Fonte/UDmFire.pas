procedure TDmFire.CadServico_RevisaoAfterInsert(DataSet: TDataSet);
begin
   DmFire.ConsSql.Close;
   DmFire.ConsSql.SQL.Clear;
   DmFire.ConsSql.SQL.Add('SELECT MAX(CODREV) AS CODREV FROM SERVICO_REVISAO WHERE CODSER = :CODSER');
   DmFire.ConsSql.ParamByName('CODSER').AsInteger := DmFire.CadServicoCODSER.AsInteger;
   DmFire.ConsSql.Open;
   if DmFire.ConsSql.IsEmpty then
      CadServico_RevisaoCODREV.AsInteger := 1
   else
      CadServico_RevisaoCODREV.AsInteger := DmFire.ConsSql.FieldByName('CODREV').AsInteger + 1;

   CadServico_RevisaoCODSER.AsInteger := DmFire.CadServicoCODSER.AsInteger;
end;

procedure TDmFire.CadServico_RevisaoBeforePost(DataSet: TDataSet);
begin
if CadServico_RevisaoORDEM_REV.AsInteger = 0 then
   begin
     Application.MessageBox('Informe a orderm da reviso','Ateno', MB_ICONWARNING+MB_OK);
     Abort;
   end;
if (CadServico_RevisaoLIMITE_KM.AsInteger = 0) and (CadServico_RevisaoLIMITE_DIAS.AsInteger = 0) then
   begin
     Application.MessageBox('Informe o Limite de Km ou o Limite de Dias','Ateno', MB_ICONWARNING+MB_OK);
     Abort;
   end;
end;

procedure TDmFire.ClientConsComissaoVlrFreteCalcFields(DataSet: TDataSet);
begin
  ClientConsComissaoVlrFretevnVlrCom.AsFloat := ClientConsComissaoVlrFreteVLRTOTAL.AsFloat * (ClientConsComissaoVlrFreteVLRCOMFIXA.AsFloat / 100);
end;

procedure TDmFire.ClientConsInfracoesMovCalcFields(DataSet: TDataSet);
begin
   ClientConsInfracoesMovvnQtdDiasIdentCondut.AsInteger := DaysBetween(ClientConsInfracoesMovDATVCT_CONDUT.AsDateTime,Date);

   if ClientConsInfracoesMovDATVCT_CONDUT.AsDateTime < date then
      ClientConsInfracoesMovvnQtdDiasIdentCondut.AsInteger := ClientConsInfracoesMovvnQtdDiasIdentCondut.AsInteger * (-1);
end;

procedure TDmFire.ClientCustoKmCalcFields(DataSet: TDataSet);
begin
  ClientCustoKmvnPedagio.Value := RoundTo(RoundTo(ClientCustoKmPEDAGIO.Value,-3) + RoundTo(ClientCustoKmVIAFACIL.Value,-3),-3);
  ClientCustoKmvnCombustivel.Value := RoundTo(RoundTo(ClientCustoKmCOMB_EMP.Value,-3) + RoundTo(ClientCustoKmCOMB_POSTO.Value,-3),-3);

  ClientCustoKmvnTotalGasto.Value := RoundTo(RoundTo(ClientCustoKmvnPedagio.Value,-3) + RoundTo(ClientCustoKmvnCombustivel.Value,-3) +
                                             RoundTo(ClientCustoKmDESCARGA.Value,-3) + RoundTo(ClientCustoKmREFEICOES.Value,-3) +
                                             RoundTo(ClientCustoKmOUTRAS_DESP.Value,-3) + RoundTo(ClientCustoKmCOMISSAO.Value,-3) +
                                             RoundTo(ClientCustoKmSALARIO.Value,-3) + RoundTo(ClientCustoKmMANUTENCOES.Value,-3),-3) +
                                             RoundTo(ClientCustoKmDEPRECIACAO.Value,-3) + RoundTo(ClientCustoKmFINANCIAMENTOS.Value,-3)+
                                             RoundTo(ClientCustoKmPNEUS.Value,-3);

 ClientCustoKmvnCustoKm.Value := RoundTo(RoundTo(ClientCustoKmvnTotalGasto.Value,-3) / ClientCustoKmTOTAL_KM.Value,-3);

 ClientCustoKmvnTotalKm.Value := ClientCustoKmTOTAL_KM.AsInteger;

end;

